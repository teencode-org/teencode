{
  "name": "teencode",
  "version": "1.0.0",
  "description": "building africa's future through tech",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "NODE_ENV='test' NODE_PATH=src mocha --reporter progress tools/testSetup.js \"src/**/*.spec.js\"",
    "test:dev": "mocha --reporter spec tools/testSetup.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "rimraf coverage/ && istanbul cover _mocha tools/testSetup.js \"src/**/*.spec.js\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test build:html",
    "build": "babel-node tools/build.js NODE_ENV=production",
    "postinstall": "npm run build",
    "test:server": "node_modules/.bin/mocha --report lcovonly \"tools/tests/**/*.test.js\""
  },
  "author": "Innocent Amadi",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.8.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.8.0",
    "babel-runtime": "6.6.1",
    "body-parser": "^1.17.2",
    "bootstrap": "4.0.0-alpha.3",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "4.0.1",
    "coveralls": "^2.11.12",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "dompurify": "1.0.1",
    "dotenv": "4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^2.9.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "^5.0.1",
