    "faker": "4.1.0",
    "file-loader": "0.8.5",
    "font-awesome": "4.6.3",
    "history": "^1.12.5",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^1.0.0-alpha.2",
    "jquery": "^3.1.1",
    "js-cookie": "2.1.3",
    "jsdom": "^9.12.0",
